@{
	Layout = null;
}
<!doctype html>
<head><title>Authentication</title>
	<meta charset="utf-8" />
	</head>
<html>
	<body>
	<section>
	<h1>Authentication</h1>
	<form method="post">
		<p>
			<label class="legend">Email</label>
			<input class = "em" name="email" />
		</p>
		<p>
			<label class="legend">Password</label>
			<input class = "pass" type="password" name="password" />
		</p>
		<div class="btns"><input class = "btnSignIn" type="submit" value="Sign in"/></div>
	</form>
	<form method="post" action="~/fido/login">
		<p>
			<label class="legend">Email</label>
			<input class = "em" name="userId" />
		</p>
		<div class="btns"><input class = "btnSignIn" type="submit" value="Sign wigh FIDO"/></div>
	</form>
	</section>
	</body>
</html>